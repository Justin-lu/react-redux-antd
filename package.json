{
  "name": "react-redux-antd",
  "version": "1.0.0",
  "description": "react redux best practice",
  "main": "index.js",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "start:prod": "npm run build && npm run open:dist",
    "start:mock": "babel-node tools/mock.js",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\"",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Justin-lu/react-redux-antd.git"
  },
  "dependencies": {
    "antd": "^3.3.1",
    "dateformat": "3.0.3",
    "es6-promise": "4.2.4",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.0",
    "jsdom": "^11.6.2",
    "lodash": "^4.16.6",
    "object-assign": "4.1.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "standard-error": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "8.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-antd": "0.5.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "browser-sync": "2.23.6",
    "chai": "4.1.2",
    "chalk": "2.3.2",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "eslint": "4.19.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-watch": "3.1.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.0.7",
    "http-proxy-middleware": "0.18.0",
    "isparta": "4.0.0",
    "json-server": "0.12.1",
    "mocha": "5.0.4",
    "nock": "9.2.3",
    "node-sass": "^4.5.2",
    "npm-run-all": "4.1.2",
    "ora": "2.0.0",
    "postcss-loader": "2.1.2",
    "react-addons-test-utils": "15.6.2",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "sinon": "4.4.6",
    "sinon-chai": "3.0.0",
    "style-loader": "0.20.3",
    "webpack": "4.1.1",
    "webpack-dev-middleware": "3.0.1",
    "webpack-hot-middleware": "2.21.2"
  },
  "author": "Justin Lu <gdjyluxiaoyong@gmail.com> (http://justin-lu.github.io/)",
  "license": "ISC"
}
